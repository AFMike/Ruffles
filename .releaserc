{
    "plugins": [
        ["@semantic-release/commit-analyzer", {
            "releaseRules": [
                {"type": "docs", "scope": "xml", "release": "patch"}
            ]
        }],
        "@semantic-release/release-notes-generator",
        ["@semantic-release/exec", {
            "analyzeCommitsCmd": "setx MACHINE SEMANTIC_LAST_VERSION ${lastRelease.version} > nul",
            "verifyReleaseCmd": "setx MACHINE SEMANTIC_NEXT_VERSION ${nextRelease.version} > nul",
            "publishCmd": "nuget push *.nupkg -ApiKey %nuget_key% -NonInteractive -Source https://api.nuget.org/v3/index.json",
        }],
        ["@semantic-release/github", {
            "assets": [
                {"path": "*net*.zip"}
            ]
        }]
    ]
}